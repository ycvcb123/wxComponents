/* winsonye start*/
.container {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    /* background: #f00 */
}

.masker {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0);
    opacity: 1;
    position: absolute;
    bottom: 0rpx;
    color: #fff;
    z-index: 2;
    transform: translateY(100%);
}

.masker-d {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgb(0,0,0);
    opacity: 0;
    position: absolute;
    bottom: 0rpx;
    color: #fff;
    z-index: 1;
}

.ad-content {
    position: absolute;
    left: 50%;
    top: 77rpx;
    transform: translateX(-50%);
    width: 650rpx;
    height: 1000rpx;
}

.ad-content-close {
    position: absolute;
    left: 50%;
    bottom: 44rpx;
    transform: translateX(-50%);
    width: 64rpx;
    height: 64rpx;
}

.ad-ticket{
    background: #fff;
    box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.1);
    position: relative;
    -webkit-mask: url(data:image/png;base64,) repeat-x 2rpx 0/53rpx 15rpx, linear-gradient(0deg, rgba(0,0,0,1) 0, rgba(0,0,0,1) 100%) no-repeat 0 14rpx/650rpx 972rpx, url(data:image/png;base64,) repeat-x 22rpx bottom/53rpx 18rpx;
    -webkit-mask-composite: add;
  }

  .dsn {
      display: none;
  }

  .button {
      position: fixed;
      top: 88rpx;
      left: 0;
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      text-align: center;
      background: #000;
      color: rgb(0, 255, 13);
  }